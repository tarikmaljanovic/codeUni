{
  "name": "codeuni",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:system": "playwright test",
    "test:system:headed": "playwright test --headed",
    "test:system:debug": "playwright test --debug",
    "test:system:ui": "playwright test --ui",
    "test:system:report": "playwright show-report tests/system/reports/html",
    "test:system:headless": "playwright test --headless",
    "test:system:ci": "playwright test --reporter=html",
    "test:system:performance": "playwright test tests/system/performance.spec.js",
    "test:regression": "jest tests/regression/",
    "test:regression:demo": "node tests/regression/regression-demo.js",
    "test:regression:quick": "node tests/regression/regression-demo.js --quick",
    "test:coverage:analysis": "node tests/coverage/coverage-analysis.js",
    "test:coverage:badges": "node tests/coverage/coverage-badge.js",
    "test:coverage:report": "npm run test:coverage && open tests/coverage/reports/lcov-report/index.html",
    "test:coverage:threshold": "npm test -- --coverage --coverageThreshold='{}' --watchAll=false",
    "test:all": "npm run test && npm run test:regression && npm run test:system:headless"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/base": "^5.0.0-beta.25",
    "@mui/icons-material": "^5.14.19",
    "@mui/joy": "^5.0.0-beta.16",
    "@mui/material": "^5.14.19",
    "axios": "^1.6.2",
    "bulma": "^0.9.4",
    "cloudinary": "^1.41.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "next": "^14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "react-quill": "^2.0.0",
    "sass": "^1.69.5",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
